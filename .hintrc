{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-parent": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-text-size-adjust",
          "-webkit-tap-highlight-color",
          "@-ms-viewport",
          "-webkit-text-decoration-skip",
          "@-ms-viewport",
          "-webkit-overflow-scrolling",
          "-webkit-overflow-scrolling",
          "-webkit-overflow-scrolling",
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 111",
    "not and_ff <= 110",
    "not chrome <= 111",
    "not edge <= 111",
    "not firefox <= 111",
    "not ios_saf <= 16.3",
    "not opera <= 95",
    "not safari <= 16.3",
    "not samsung <= 20"
  ]
}